{
  "name": "larva/laravel-forum",
  "description": "This is a forum.",
  "keywords": [ "laravel", "forum", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "illuminate/database": "^5.7 || ^6.0 || ^7.0",
    "illuminate/support": "^5.7 || ^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Forum\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Larva\\Forum\\ForumServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
